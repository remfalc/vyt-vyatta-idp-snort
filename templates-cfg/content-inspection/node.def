priority: 230 # before inspect is enabled on interfaces/zones
help: Content Inspection settings [DEPRECATED]
end: RET1=1
     echo "WARNING: IPS is deprecated!"
     echo "Snort rules may no longer update properly."
     echo
     if [ -x /opt/vyatta/sbin/vyatta-update-antivirus.pl ]; then
       sudo /opt/vyatta/sbin/vyatta-update-antivirus.pl
       RET1=$?
     fi
     export VYATTA_BOOTING=$BOOT
     sudo /opt/vyatta/sbin/vyatta-update-snort.pl
     RET2=$?
     sudo /opt/vyatta/sbin/vyatta-update-inspection.pl $RET1 $RET2
 
